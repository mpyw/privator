{
    "name": "mpyw/privator",
    "description": "Utils for testing private methods and properties on PHP 7.0+",
    "type": "library",
    "license": "MIT",
    "keywords": ["test", "private"],
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Mpyw\\Privator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\Privator\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!/src/*"
        ]
    }
}
